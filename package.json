{
  "name": "netlify-ts",
  "version": "1.4.0",
  "description": "Turn your Netlify CMS collections into TypeScript typings!",
  "repository": "jsaari97/netlify-ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Jim Saari",
  "keywords": [
    "netlify",
    "typescript",
    "types",
    "generate"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "bin": "./bin/run",
  "files": [
    "/bin",
    "/lib"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --fix",
    "kitchen-sink": "./bin/run kitchen-sink.yml",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "js-yaml": "^4.0.0",
    "ora": "^5.3.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.0",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.2.1",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
